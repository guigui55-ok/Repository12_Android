<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.sampleapplication01.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.sampleapplication01.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.sampleapplication01.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_count_up"
        android:name="com.example.sampleapplication01.ui.countup.CountUpFragment"
        android:label="CountUpDown"
        tools:layout="@layout/fragment_count_up" />

    <fragment
        android:id="@+id/navigation_temp"
        android:name="com.example.sampleapplication01.ui.temp_screen.FragmentTemp"
        android:label="Temp Screen"
        tools:layout="@layout/fragment_temp" />
</navigation>